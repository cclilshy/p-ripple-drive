{
  "name": "cloudtay/ripple-driver",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ripple\\Driver\\": "src/",
      "Tests\\": "tests/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "authors": [
    {
      "name": "cclilshy",
      "email": "jingnigg@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-sockets": "*",
    "ext-pdo": "*",
    "cloudtay/ripple": "^1.0.0",
    "cloudtay/ripple-http": "dev-main",
    "cloudtay/ripple-websocket": "dev-main"
  },
  "require-dev": {
    "amphp/mysql": "^3.0",
    "friendsofphp/php-cs-fixer": "*",
    "illuminate/http": "*",
    "illuminate/console": "*",
    "illuminate/container": "*",
    "illuminate/support": "*",
    "laravel/framework": "*",
    "phpunit/phpunit": "*",
    "symfony/console": "*",
    "topthink/framework": "*",
    "workerman/workerman": "*",
    "workerman/webman-framework": "dev-master",
    "yiisoft/yii2": "^2.0@beta"

  },
  "extra": {
    "laravel": {
      "providers": [
        "Ripple\\Driver\\Laravel\\Provider"
      ]
    },
    "think": {
      "services": [
        "Ripple\\Driver\\ThinkPHP\\Service"
      ]
    },
    "yii\\bootstrap": "Ripple\\Driver\\Yii2\\Bootstrap"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
